{
  "minify": {
    "mangle": {
      "properties": {
        "regex": "^_[^_]"
      }
    },
    "compress": {
      "inline": false,
      "reduce_funcs": false
    }
  },
  "props": {
    "props": {
      "$_left": "l",
      "$_middle": "m",
      "$_right": "r",
      "$_tree": "t",
      "$_thunk": "f",
      "$_node": "n",
      "$_digit": "d"
    }
  }
}